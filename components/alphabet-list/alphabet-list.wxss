
.alphabet-list-wrapper{
  position: relative;
  padding: 10rpx 0 10rpx 30rpx;
}

.section-item-header {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: red;
  padding-bottom: 20rpx;
}

.alphanet-selector {
  position: absolute;
  top: 50%;
  right: 2px;
  transform: translateY(-50%);
}

.selector-one {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
	color: #333;
  height: 28rpx;
  width: 28rpx;
  margin-right: 30rpx;
}

